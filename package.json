{
  "name": "ghis",
  "version": "0.0.0",
  "scripts": {
    "404": " cd dist/browser &&  dir && cp index.html 404.html",
    "ng": "ng",
    "watch": "ng build --watch --configuration development",
    "build:github-pageXXXX": "ng build --configuration production --output-path dist/ --base-href ./ && npm run 404",
    "build": "npm run clean  && npm run build:scss && npm run build:scripting && npm run build:assets",
    "build:assets": "node scripting/build-assets.js",
    "build:scripting": "node scripting/build-js.js",
    "build:scss": "node scripting/build-scss.js",
    "clean": "node scripting/clean.js",
    "start": "npm run build && node scripting/start.js",
    "start:debug": "npm run build && node scripting/start-debug.js"
  },
  "description": "Resume is a simple, yet elegant resume and CV theme for Bootstrap 4.",
  "keywords": [
    "css",
    "sass",
    "html",
    "responsive",
    "theme",
    "template"
  ],
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "bootstrap": "5.2.3"
  },
  "devDependencies": {
    "autoprefixer": "10.4.14",
    "browser-sync": "^3.0.4",
    "chokidar": "3.5.3",
    "concurrently": "6.3.0",
    "postcss": "^8.5.6",
    "prettier": "2.8.6",
    "pug": "^3.0.3",
    "sass": "1.60.0",
    "shelljs": "0.8.5",
    "upath": "2.0.1"
  }
}