{
  "name": "ghis",
  "version": "0.0.0",
  "scripts": {
    "build": "npm run clean  && npm run build:scss && npm run build:js && npm run build:assets && npm run build:html",
    "build:assets": "node scripting/build-assets.js",
    "build:js": "node scripting/build-js.js",
    "build:scss": "node scripting/build-scss.js",
    "clean": "node scripting/clean.js",
    "build:html": "node scripting/build-html.js",
    "dev": "concurrently \"npm run watch:scss\" \"npm run serve\"",
    "serve": "browser-sync start --server 'dist' --files 'dist'",
    "watch:scss": "chokidar 'src/scss/**/*.scss' -c 'npm run build:scss'"
  },
  "description": "Elegant resume and CV theme with Bootstrap 5.",
  "keywords": [
    "css",
    "sass",
    "html",
    "responsive",
    "theme",
    "template"
  ],
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "bootstrap": "5.2.3"
  },
  "devDependencies": {
    "autoprefixer": "10.4.14",
    "browser-sync": "^3.0.4",
    "chokidar": "3.5.3",
    "concurrently": "6.3.0",
    "postcss": "^8.5.6",
    "prettier": "2.8.6",
    "pug": "^3.0.3",
    "sass": "1.60.0",
    "shelljs": "0.8.5",
    "upath": "2.0.1"
  }
}